{
  "version": "0.0.1",
  "scripts": {
    "start": "node starter",
    "develop": "NODE_ENV=development nodemon -i node_modules/ --exec \"npm run develop:run\"",
    "develop:run": "npm run lint && node babelhook",
    "lint": "eslint ./"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "parser": "babel-eslint",
    "rules": {
      "quotes": [
        2,
        "single",
        "avoid-escape"
      ],
      "comma-dangle": [
        0
      ],
      "no-underscore-dangle": [
        0
      ],
      "no-empty": [
        0
      ],
      "strict": [
        2,
        "never"
      ]
    }
  },
  "dependencies": {
    "mysql": "^2.3.2",
    "express": "^4.3.2",
    "nconf": "^0.6.9",
    "async": "^0.9.0",
    "lodash": "^2.4.1",
    "body-parser": "^1.2.2",
    "sync-prompt": "^0.3.3-beta",
    "bcrypt": "^0.7.8",
    "mkdirp": "^0.5.0",
    "redis-sessions": "^0.3.4",
    "cors": "^2.3.1",
    "jade": "^1.3.1",
    "nedb": "^0.10.5",
    "moment": "^2.7.0"
  },
  "devDependencies": {
    "babel": "^5.6.14",
    "babel-eslint": "^3.1.19",
    "eslint": "^0.24.0",
    "mocha": "^1.20.1"
  }
}
